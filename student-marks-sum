include<stdio.h>
#include<stdlib.h>

int marks_summation(int*marks,int number_of_students,char gender)
{
  int sum=0,i,j;
  if(gender=='b')
  
}
   {
     for(i=0; i<number_of_students;i=i+2)
     {
       sum =sum+marks[i];
       
     }
   }
    else if(gender =='g')
    {
      for(j=1; i<number_of_students;i=i+2)
      {
        sum =sum+marks[j];
      }
    }
      
    return sum;
}
